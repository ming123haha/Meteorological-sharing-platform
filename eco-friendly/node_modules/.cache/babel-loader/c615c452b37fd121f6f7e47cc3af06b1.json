{"ast":null,"code":"// @ is an alias to /src\nimport HelloWorld from '@/components/HelloWorld.vue';\nimport request from \"@/utils/request\";\nexport default {\n  data() {\n    return {\n      tableData: [],\n      dialogFormVisible: false,\n      total: 0,\n      pageNum: 1,\n      pageSize: 5,\n      username: \"\",\n      password: \"\",\n      form: {},\n      collapseBtnClass: 'el-icon-s-fold',\n      isCollapsed: false,\n      sideWidth: 200,\n      logoTextShow: true,\n      headerBg: 'headerBg'\n    };\n  },\n\n  created() {\n    //请求分析查询数据\n    this.load();\n  },\n\n  methods: {\n    collapse() {\n      //点击收缩触发函数\n      this.isCollapsed = !this.isCollapsed;\n\n      if (this.isCollapsed) {\n        this.sideWidth = 64;\n        this.collapseBtnClass = 'el-icon-s-unfold';\n        this.logoTextShow = false;\n      } else {\n        this.logoTextShow = true;\n        this.sideWidth = 200;\n        this.collapseBtnClass = 'el-icon-s-fold';\n      }\n    },\n\n    handleAdd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n\n    load() {\n      //加载\n      request.get(\"/user/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          username: this.username,\n          password: this.password\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.records;\n        this.total = res.total;\n      });\n    },\n\n    save() {\n      //保存\n      request.post(\"/user\", this.form).then(res => {\n        if (res) {\n          this.$message.success(\"保存成功!\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败!\");\n        }\n      });\n    },\n\n    del(id) {\n      //删除\n      request.delete(\"/user/\" + id).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功!\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"删除失败!\");\n        }\n      });\n    },\n\n    reset() {\n      this.username = \"\";\n      this.password = \"\";\n      this.load();\n    },\n\n    handleSizeChange(pageSize) {\n      //页面最大条数\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //第几页\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyHA;AACA;AACA;AAEA;EACAA;IACA;MACAC,aADA;MAEAC,wBAFA;MAGAC,QAHA;MAIAC,UAJA;MAKAC,WALA;MAMAC,YANA;MAOAC,YAPA;MAQAC,QARA;MASAC,kCATA;MAUAC,kBAVA;MAWAC,cAXA;MAYAC,kBAZA;MAaAC;IAbA;EAeA,CAjBA;;EAkBAC;IACA;IACA;EACA,CArBA;;EAsBAC;IACAC;MACA;MACA;;MACA;QACA;QACA;QACA;MACA,CAJA,MAIA;QACA;QACA;QACA;MACA;IACA,CAbA;;IAcAC;MACA;MACA;IACA,CAjBA;;IAkBAC;MAAA;MACAC;QACAC;UACAhB,qBADA;UAEAC,uBAFA;UAGAC,uBAHA;UAIAC;QAJA;MADA,GAOAc,IAPA,CAOAC;QACAC;QACA;QACA;MACA,CAXA;IAYA,CA/BA;;IAgCAC;MAAA;MACAL;QACA;UACA;UACA;UACA;QACA,CAJA,MAIA;UACA;QACA;MACA,CARA;IASA,CA1CA;;IA2CAM;MAAA;MACAN;QACA;UACA;UACA;UACA;QACA,CAJA,MAIA;UACA;QACA;MACA,CARA;IASA,CArDA;;IAsDAO;MACA;MACA;MACA;IACA,CA1DA;;IA2DAC;MAAA;MACAJ;MACA;MACA;IACA,CA/DA;;IAgEAK;MAAA;MACAL;MACA;MACA;IACA;;EApEA;AAtBA","names":["data","tableData","dialogFormVisible","total","pageNum","pageSize","username","password","form","collapseBtnClass","isCollapsed","sideWidth","logoTextShow","headerBg","created","methods","collapse","handleAdd","load","request","params","then","res","console","save","del","reset","handleSizeChange","handleCurrentChange"],"sourceRoot":"src/views","sources":["HomeView.vue"],"sourcesContent":["<template>\n  <el-container style=\"height: 100vh\">\n    <el-aside :width=\"sideWidth + 'px'\" style=\"background-color: rgb(238, 241, 246); box-shadow: 2px 0 6px rgb(43,51,62)\">\n      <!-- 此处会造成菜单栏卡顿 -->\n      <el-menu :default-openeds=\"['1', '3']\"\n               style=\"min-height: 100% ;active-text-color:blue\"\n               :collapse-transition=\"true\"\n               :collapse=\"isCollapsed\">\n\n        <div style=\"height: 60px;line-height:60px;text-align: center\">\n          <img src=\"../assets/logo.png\" alt=\"\" style=\"width: 20px;position: relative;top:5px;margin-right: 3px\">\n          <b style=\"color: #93b5cf\" v-show=\"logoTextShow\">智慧大气环境监测系统</b>\n        </div>\n        <el-submenu index=\"1\">\n          <template slot=\"title\"><i class=\"el-icon-loading\"></i>\n            <span slot=\"title\">首页</span>\n          </template>\n          <el-menu-item-group>\n           <el-menu-item index=\"1-1\"><i class=\"el-icon-map-location\" style=\"position:relative\"></i>地图大屏</el-menu-item>\n           <el-menu-item index=\"1-2\"><i class=\"el-icon-edit-outline\" style=\"position: relative\"></i>数据管理</el-menu-item>\n          </el-menu-item-group>\n        </el-submenu>\n        <el-submenu index=\"2\">\n          <template slot=\"title\"><i class=\"el-icon-menu\"></i>\n            <span slot=\"title\">导航二</span>\n          </template>\n        </el-submenu>\n        <el-submenu index=\"3\">\n          <template slot=\"title\"><i class=\"el-icon-setting\"></i>\n            <span slot=\"title\">导航三</span>\n          </template>\n        </el-submenu>\n      </el-menu>\n    </el-aside>\n\n    <el-container>\n      <el-header style=\" font-size: 12px ; display:flex\">\n        <div style=\"flex: 1; font-size: 18px\">\n          <span :class=\"collapseBtnClass\" style=\"cursor: pointer\"@click=\"collapse\"></span>\n        </div>\n        <el-dropdown style=\"width: 65px; cursor:pointer\">\n          <span>王小虎</span><i class=\"el-icon-arrow-down\" style=\"margin-left: 5px\"></i>\n          <el-dropdown-menu slot=\"dropdown\">\n            <el-dropdown-item>退出</el-dropdown-item>\n          </el-dropdown-menu>\n        </el-dropdown>\n      </el-header>\n\n      <el-main>\n\n        <div style=\"margin: 10px 0\">\n          <el-input style=\"width: 200px\" placeholder=\"请输入你想搜索的内容\" suffix-icon=\"el-icon-search\" v-model=\"username\"></el-input>\n          <el-input style=\"width: 200px\" placeholder=\"请输入密码\" suffix-icon=\"el-icon-message\" class=\"ml-5\" v-model=\"password\"></el-input>\n          <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\n          <el-button type=\"warning\" @click=\"reset\">重置</el-button>\n        </div>\n        <div style=\"margin: 10px 0\">\n          <el-button type=\"primary\" @click=\"handleAdd\">新增 <i class=\"el-icon-circle-plus-outline\"></i> </el-button>\n          <el-button type=\"danger\">批量删除 <i class=\"el-icon-remove-outline\"></i> </el-button>\n          <el-button type=\"primary\">导入 <i class=\"el-icon-bottom\"></i> </el-button>\n          <el-button type=\"primary\">导出 <i class=\"el-icon-top\"></i> </el-button>\n        </div>\n\n        <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\">\n          <el-table-column prop=\"id\" label=\"ID\" width=\"80\">\n          </el-table-column>\n          <el-table-column prop=\"username\" label=\"用户名\" width=\"140\">\n          </el-table-column>\n          <el-table-column prop=\"password\" label=\"密码\" width=\"120\">\n          </el-table-column>\n          <el-table-column label=\"操作\" >\n            <template slot-scope=\"scope\">\n              <el-button type=\"success\">编辑 <i class=\"el-icon-edit\"></i> </el-button>\n              <el-popconfirm\n                  title=\"确定删除吗？\"\n                  confirm-button-text='确定'\n                  cancel-button-text='我再想想'\n                  icon=\"el-icon-info\"\n                  icon-color=\"red\"\n                  @confirm=\"del(scope.row.id)\"\n              >\n              </el-popconfirm>\n              <el-button type=\"danger\" @click=\"del(scope.row.id)\">删除<i class=\"el-icon-remove-outline\"></i> </el-button>\n            </template>\n          </el-table-column>\n        </el-table>\n        <div style=\"padding: 10px 0\">\n          <el-pagination\n              @size-change=\"handleSizeChange\"\n              @current-change=\"handleCurrentChange\"\n              :current-page=\"pageNum\"\n              :page-sizes=\"[2, 5, 10, 20]\"\n              :page-size=\"pageSize\"\n              layout=\"total, sizes, prev, pager, next, jumper\"\n              :total=\"total\">\n          </el-pagination>\n        </div>\n\n        <el-dialog title=\"信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\" >\n          <el-form label-width=\"80px\" size=\"small\">\n            <el-form-item label=\"用户名\">\n              <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n            <el-form-item label=\"密码\">\n              <el-input v-model=\"form.password\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n          </el-form>\n          <div slot=\"footer\" class=\"dialog-footer\">\n            <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n          </div>\n        </el-dialog>\n\n\n      </el-main>\n\n    </el-container>\n  </el-container>\n</template>\n\n<script>\n// @ is an alias to /src\nimport HelloWorld from '@/components/HelloWorld.vue'\nimport request from \"@/utils/request\";\n\nexport default {\n  data() {\n    return {\n      tableData:[],\n      dialogFormVisible:false,\n      total:0,\n      pageNum:1,\n      pageSize:5,\n      username:\"\",\n      password:\"\",\n      form:{},\n      collapseBtnClass: 'el-icon-s-fold',\n      isCollapsed:false,\n      sideWidth:200,\n      logoTextShow:true,\n      headerBg:'headerBg'\n    }\n  },\n  created() {\n    //请求分析查询数据\n    this.load()\n  },\n  methods: {\n    collapse() {\n      //点击收缩触发函数\n      this.isCollapsed = !this.isCollapsed\n      if (this.isCollapsed) {\n        this.sideWidth = 64\n        this.collapseBtnClass = 'el-icon-s-unfold'\n        this.logoTextShow = false\n      } else {\n        this.logoTextShow = true\n        this.sideWidth = 200\n        this.collapseBtnClass = 'el-icon-s-fold'\n      }\n    },\n    handleAdd(){\n      this.dialogFormVisible=true\n      this.form={}\n    },\n    load() {  //加载\n      request.get(\"/user/page\",{\n        params:{\n          pageNum:this.pageNum,\n          pageSize:this.pageSize,\n          username:this.username,\n          password:this.password\n        }\n      }).then(res =>{\n        console.log(res)\n        this.tableData=res.records\n        this.total=res.total\n      })\n    },\n    save(){  //保存\n      request.post(\"/user\",this.form).then(res=>{\n        if(res){\n          this.$message.success(\"保存成功!\")\n          this.dialogFormVisible=false\n          this.load()\n        }else{\n          this.$message.error(\"保存失败!\")\n        }\n      })\n    },\n    del(id){  //删除\n      request.delete(\"/user/\"+id).then(res=>{\n        if(res){\n          this.$message.success(\"删除成功!\")\n          this.dialogFormVisible=false\n          this.load()\n        }else{\n          this.$message.error(\"删除失败!\")\n        }\n      })\n    },\n    reset(){\n      this.username=\"\"\n      this.password=\"\"\n      this.load()\n    },\n    handleSizeChange(pageSize) {    //页面最大条数\n      console.log(pageSize)\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {   //第几页\n      console.log(pageNum)\n      this.pageNum = pageNum\n      this.load()\n    }\n  }\n}\n</script>\n<style>\n.headerBg{\n  background: #eee!important;\n}\n.el-header {\n  background-color: #B3C0D1;\n  color: #333;\n  line-height: 60px;\n}\n\n.el-aside {\n  color: #333;\n}\n</style>"]},"metadata":{},"sourceType":"module"}