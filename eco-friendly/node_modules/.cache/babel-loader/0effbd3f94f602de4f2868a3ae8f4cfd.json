ox-group>\r\n\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"> <!--多选框-->\r\n      </el-table-column>\r\n      <el-table-column prop=\"AQI\" label=\"AQI\" width=\"150\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"CO\" label=\"CO\" width=\"200\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"NO2\" label=\"NO2\" width=\"200\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"PM25\" label=\"PM25\" width=\"200\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"PM10\" label=\"PM10\" width=\"200\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"SO2\" label=\"SO2\" width=\"200\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"O3\" label=\"O3\" width=\"200\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"city\" label=\"city\" width=\"200\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"station\" label=\"station\" width=\"180\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"pubtime\" label=\"pubtime\" width=\"180\">\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[ 5, 10]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n\r\n</div>\r\n</template>\r\n\r\n<script>\r\nconst cityOptions = ['AQI', 'CO', 'NO2', 'PM2.5','PM10','SO2','O3'];\r\nexport default {\r\n  name: \"data\",\r\n  data() {\r\n    return {\r\n      pageNum:1,\r\n      pageSize:15,\r\n      tableData:[],\r\n      headerBg:'headerBg',\r\n      input: '',\r\n      checkAll: false,\r\n      checkedCities: ['上海', '北京'],\r\n      cities: cityOptions,\r\n      isIndeterminate: true\r\n    };\r\n  },\r\n  created() {\r\n  },\r\n  methods: {\r\n    load(){\r\n\r\n    },\r\n    handleSizeChange(){\r\n\r\n    },\r\n    handleCurrentChange(){\r\n\r\n    },\r\n    handleCheckAllChange(val) {\r\n      this.checkedCities = val ? cityOptions : [];\r\n      this.isIndeterminate = false;\r\n    },\r\n    handleCheckedCitiesChange(value) {\r\n      let checkedCount = value.length;\r\n      this.checkAll = checkedCount === this.cities.length;\r\n      this.isIndeterminate = checkedCount > 0 && checkedCount < this.cities.length;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.el-input {\r\nwidth: 300px;\r\n  margin: 10px 10px;\r\n}\r\n.el-table{\r\n  margin-left: 10px;\r\n  margin-top: 10px;\r\n  width:1900px;\r\n}\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}