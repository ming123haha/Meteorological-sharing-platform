r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n      <div class=\"panel\">\r\n        <div>\r\n          <div class=\"chart\" >\r\n            <echartspie></echartspie>\r\n          </div>\r\n        </div>\r\n        </div>\r\n      <div class=\"panel\">\r\n        <div>\r\n          <echartsbar></echartsbar>\r\n        </div>\r\n    </div>\r\n      </div>\r\n    <div class=\"column\">\r\n      <div class=\"panel\" style=\"height: 100%;width: 100%;\">\r\n      </div>\r\n    </div>\r\n    <div class=\"column\">\r\n<!--折线图-->\r\n      <div class=\"panel bar\">\r\n        <echartsline></echartsline>\r\n      </div>\r\n<!--雷达图-->\r\n      <div class=\"panel radar\">\r\n        <scatter></scatter>\r\n      </div>\r\n      </div>\r\n  </section>\r\n\r\n</template>\r\n\r\n<script>\r\n// import tabsection from \"@/components/tabsection\";\r\nimport echartspie from \"@/components/echartspie\";\r\nimport echartsline from \"@/components/echartsline\";\r\nimport scatter from \"@/components/scatter\"\r\nimport echartsbar from \"@/components/echartsbar\"\r\nexport default {\r\n  name: \"Home\",\r\n  data() {\r\n    return {\r\n      airgood:null,\r\n      airnormal:null,\r\n      airbad:null\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.initdata()\r\n  },\r\n  //引入我的AQI仪表盘组件\r\n  components:{\r\n    echartspie :echartspie,\r\n    // tabsection:tabsection,\r\n    echartsline:echartsline,\r\n    scatter:scatter,\r\n    echartsbar:echartsbar,\r\n  },\r\n  methods: {\r\n    initdata(){\r\n\r\n\r\n      this.airgood=40\r\n      this.airnormal=30\r\n      this.airbad=14\r\n    }\r\n  }\r\n}\r\n\r\n\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.mainbox {\r\n  display: flex;\r\n  /*position:relative;*/\r\n  min-width:100%;\r\n  max-height: 100%;\r\n  margin: 0 ;\r\n  /*padding: 10px 10px 0;*/\r\n  /* background-image: url(../assets//images/bgc.jpg);\r\n  background-size: cover; */\r\n}\r\n.mainbox .column {\r\n  flex: 4rem;\r\n}\r\n.mainbox .column:nth-child(2) {\r\n  flex: 5;\r\n  margin: 0 0.125rem 0.1875rem;\r\n  overflow: hidden;\r\n}\r\n/* 利用边框图片切割 */\r\n.mainbox .panel{\r\n  position: relative;\r\n  border: 15px solid transparent;\r\n  border-width: 30px 20px 30px 20px;\r\n  border-image-source: url(../assets/images/blue.png);\r\n  border-image-slice: 78 45 78 45;\r\n  border-image-width: 20px;\r\n}\r\n 让子容器撑开覆盖父容器\r\n.panel .inner{\r\n  position:absolute;\r\n  /*top:-78px;*/\r\n  /*left:-45px;*/\r\n  /*bottom:-78px;*/\r\n  /*right:-45px;*/\r\n  padding:10px 10px;\r\n  background-color: #fff;\r\n}\r\n.mainbox.column.overview {\r\n  height:200px;\r\n}\r\n.mainbox ul{\r\n  display:flex;\r\n  justify-content:space-between;\r\n  white-space:nowrap;\r\n}\r\n.mainbox ul li span {\r\n  font-size: 15px;\r\n  color:yellow;\r\n  text-align:center;\r\n}\r\n.mainbox ul li h4 {\r\n  font-size:20px;\r\n  color:aliceblue;\r\n  text-align:center;\r\n}\r\ntable {\r\n  border-collapse: collapse;\r\n  color: white;\r\n  width: 100%;\r\n}\r\n\r\ntable, td, th {\r\n  border: transparent;\r\n  height: 40px;\r\n  text-align:center;\r\n}\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}